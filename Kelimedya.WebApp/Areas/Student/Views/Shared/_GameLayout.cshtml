@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="tr">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>
    <link href="~/theme/tabler/dist/css/tabler.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/izitoast/dist/css/iziToast.min.css" />
    @RenderSection("Head", required: false)
</head>
<body class="theme-light" style="background:#fff;">
    @RenderBody()

    <script src="https://cdn.jsdelivr.net/npm/izitoast/dist/js/iziToast.min.js"></script>
    <script src="~/js/CustomIziToast.js"></script>
    <script src="~/theme/tabler/dist/js/tabler.min.js" defer></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
